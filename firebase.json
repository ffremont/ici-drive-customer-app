{
  "hosting": {
    "target": "customer-ici-drive-prod",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source": "**/*",
      "headers": [ {
        "key": "X-Robots-Tag",
        "value": "noindex"
      } ]
    }],
    "rewrites": [{
      "source": "/api/**",
      "function": "api"
    },{
      "source": "**",
      "destination": "/index.html"
    }]
  },
    "functions": {
      "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build"
    }
  }